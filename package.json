{
  "name": "derf",
  "version": "3.0.0",
  "description": "A javascript performance debugger.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "DEBUG=test mocha test",
    "prepublish": "in-publish && npm run test && npm run build || not-in-publish",
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "postpublish": "git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azuqua/derf"
  },
  "bugs": {
    "url": "http://github.com/azuqua/derf/issues"
  },
  "keywords": [
    "performance",
    "debug",
    "debugging",
    "speed",
    "timing",
    "nanoseconds",
    "log",
    "decorator"
  ],
  "author": {
    "name": "Nicholas Clawson",
    "email": "nickclaw@gmail.com",
    "url": "nickclaw.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "lodash": "^4.12.0",
    "mimic-fn": "^1.1.0",
    "on-finished": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "eslint": "^2.10.2",
    "glob": "^7.0.3",
    "in-publish": "^2.0.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
}
